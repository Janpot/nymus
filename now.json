{
  "name": "nymus",
  "builds": [
    {
      "src": "docs/next.config.js",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "docs/$1",
      "continue": true
    }
  ],
  "redirects": [
    { "source": "/docs", "destination": "/docs/getting-styarted", "statusCode": 308 }
  ]
}
